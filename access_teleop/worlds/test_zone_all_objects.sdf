    <model name="cube_s">
      <link name='cube_s'>
        <self_collide>false</self_collide>
        <pose>3.8 3.15 0.83 0 0 0</pose>

        <collision name='cube_s'>
          <geometry>
            <box><size>0.025 0.025 0.025</size></box>
