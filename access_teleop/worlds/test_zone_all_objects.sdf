        <collision name='collision'>
          <geometry>
            <box>
              <size>0.050 0.050 0.050</size>
            </box>
          </geometry>
          <surface>
            <friction>
              <ode>
                <mu>30.0</mu>
                <mu2>30.0</mu2>
              </ode>
            </friction>
            <contact>
              <ode>
                <kp>1000000.0</kp>
                <kd>100.0</kd>
